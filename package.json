{
  "name": "hexo-site",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "git@github.com:rollawaypoint/rollawaypoint.github.io.git"
  },
  "scripts": {
    "start": "npx hexo clean && npx hexo generate",
    "dev": "npx hexo generate -w & hexo s",
    "index": "export HEXO_ALGOLIA_INDEXING_KEY='668e8865f5d99b1e6d6e5f8dbb3ee25d' && npx hexo algolia",
    "deploy": "npx hexo deploy",
    "deploy-index": "npm run start && npx hexo deploy && npm run index",
    "dep": "hexo clean && hexo generate && hexo d -g"
  },
  "hexo": {
    "version": "3.8.0"
  },
  "dependencies": {
    "firebase-admin": "^6.4.0",
    "hexo": "^3.7.1",
    "hexo-algolia": "^1.2.5",
    "hexo-deployer-git": "^0.3.1",
    "hexo-generator-archive": "^0.1.5",
    "hexo-generator-category": "^0.1.3",
    "hexo-generator-feed": "^1.2.2",
    "hexo-generator-index": "^0.2.1",
    "hexo-generator-json-content": "^3.0.1",
    "hexo-generator-seo-friendly-sitemap": "0.0.22",
    "hexo-generator-tag": "^0.2.0",
    "hexo-offline": "^1.0.0",
    "hexo-renderer-ejs": "^0.3.1",
    "hexo-renderer-marked": "^0.3.2",
    "hexo-renderer-stylus": "^0.3.3",
    "hexo-server": "^0.3.1",
    "hexo-wordcount": "^6.0.1"
  }
}